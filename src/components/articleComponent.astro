---
import PageLayout from '../layouts/PageLayout.astro'
import { Markdown } from 'astro/components'
const { content } = Astro.props
---
<PageLayout content={content}>
    <section>
            <h2>{content.title}</h2>
        <slot />
    </section>
</PageLayout>

<style>
    section {
        @apply prose
    }
</style>